  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT 
    dd.Nombre_Departamento,
    COUNT(fe.FactEsquelas_ID) AS NumeroDeEsquelas,
    SUM(fe.Ves_Valor) AS ValorTotalEsquelas,
    SUM(fe.Ves_Interes) AS ValorTotalIntereses
FROM 
    FactEsquelas fe
INNER JOIN DimDepartamento dd ON fe.Departamento_ID = dd.Departamento_ID
GROUP BY 
    dd.Nombre_Departamento
ORDER BY 
    ValorTotalEsquelas DESC;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
