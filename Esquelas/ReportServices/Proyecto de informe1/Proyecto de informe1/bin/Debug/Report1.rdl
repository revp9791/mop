  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT 
    df.Año,
    df.Mes,
    df.NombreMes,
    SUM(fe.Ves_Interes) AS InteresTotal
FROM 
    FactEsquelas fe
INNER JOIN DimFecha df ON fe.Fecha_ID = df.Fecha_ID
GROUP BY 
    df.Año,
    df.Mes,
    df.NombreMes
ORDER BY 
    df.Año,
    df.Mes;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
